page {
  height: 100%;
}
.menu {
  height: calc(100vh - 90rpx);
  display: flex;
}
.menu .left_menu {
  flex: 2;
}
.menu .left_menu .menu_item {
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.menu .left_menu .active {
  color: var(--themeColor);
  border-left: 5rpx solid currentColor;
}
.menu .right_content {
  flex: 5;
}
.menu .right_content .good_group .good_title {
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.menu .right_content .good_group .good_title .delimiter {
  color: #ccc;
  padding: 0 10rpx;
}
.menu .right_content .good_group .good_list {
  display: flex;
  flex-wrap: wrap;
}
.menu .right_content .good_group .good_list navigator {
  padding: 10rpx;
  width: 33.33%;
  text-align: center;
  font-size: 23rpx;
}
.menu .right_content .good_group .good_list navigator image {
  width: 50%;
}
